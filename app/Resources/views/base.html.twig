<!DOCTYPE html>
<html id="{% block html_id %}almacen{% endblock %}">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Bienvenido!{% endblock %} | Almacen</title>
        <meta content="" name="ALMACEN">
        <meta content="" name="SSA">
        <meta content=
            "width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"
            name="viewport"
        >
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body role="document">
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
        {% block flash_messages %}
            {# Mensajes Flash #}
            {% for flashMessage in app.session.flashbag.get('success') %}
                <script type="text/javascript">
                    $(document).ready(function(){
                        
                        createAlert("success", null, "{{flashMessage}}");
                    });
                </script>
            {% endfor %}
            {% for flashMessage in app.session.flashbag.get('danger') %}
                <script type="text/javascript">
                    $(document).ready(function(){
                        createAlert("danger", null, "{{flashMessage}}");
                    });
                </script>
            {% endfor %}
        {% endblock %}
    </body>
</html>
