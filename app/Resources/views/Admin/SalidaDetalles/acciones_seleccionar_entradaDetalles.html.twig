<script type="text/javascript">
        $("#lista-entradaDetalles").on("click", ".seleccionar-entradaDetalle", function(){
            articuloClave = $("#salida_detalles_type_articulo").val();
            entrada = $(this).attr("entrada-id");
            $("#salida_detalles_type_entradaDetalle").val(entrada);
            mostrarInfoArticulo(articuloClave,entrada);
            $("#modal-buscar-entradaDetalles").modal("hide");
            $("#modal-popup-buscar-articulo").modal("hide");
        });
</script>